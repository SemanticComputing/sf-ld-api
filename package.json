{
  "name": "sf-ld-api",
  "version": "0.0.1",
  "description": "Semantic Finlex Linked Data API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src -e js,jsx --exec \"babel-node src --presets es2015,react,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,react,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "apoksane <arttu.oksanen@aalto.fi>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "history": "<4.0.0",
    "immutable": "^3.8.1",
    "morgan": "^1.8.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "<4.0.0",
    "redux": "^3.6.0",
    "resource-router-middleware": "^0.6.0",
    "superagent": "^3.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
